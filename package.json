  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.0.3",
    "tap": "^19.2.5"
  },
  "dependencies": {
    "@mswjs/interceptors": "0.23.0",
    "yaml": "^2.8.0"
  },
  "files": [
    "dist",
    "!dist/**/*.spec.*"
  ]
